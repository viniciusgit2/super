    "build7": "./node_modules/.bin/webpack server --mode=development ",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch"
  },
  "preferGlobal": true,
  "bin": {
